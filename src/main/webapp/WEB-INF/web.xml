<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>stockPapeleria</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>SproductoPapeleria</display-name>
		<servlet-name>SproductoPapeleria</servlet-name>
		<servlet-class>stockPapeleria.Servlet.SproductoPapeleria</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SproductoPapeleria</servlet-name>
		<url-pattern>/SproductoPapeleria</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>SproductoEdit</display-name>
		<servlet-name>SproductoEdit</servlet-name>
		<servlet-class>stockPapeleria.Servlet.SproductoEdit</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SproductoEdit</servlet-name>
		<url-pattern>/SproductoEdit</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<page-encoding>UTF-8</page-encoding>
		</jsp-property-group>
	</jsp-config>
</web-app>